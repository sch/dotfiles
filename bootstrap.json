{
    "after-scripts": [
        "brew linkapps",
        "apm stars --install"
    ],
    "symlinks": {
        "atom/*": "~/.atom/",
        "bash/.*": "~",
        "bin/*": "~/.bin/",
        "emacs/spacemacs": "~/.spacemacs",
        "fish/*.fish": "~/.config/fish/",
        "fish/functions/*": "~/.config/fish/functions/",
        "git/.*": "~",
        "mutt/muttrc": "~/.muttrc",
        "pow/.*": "~",
        "ruby/.*": "~",
        "tmux/tmux.conf": "~/.tmux.conf"
    },
    "taps": [
        "caskroom/cask",
        "caskroom/fonts",
        "caskroom/versions",
        "homebrew/core",
        "homebrew/services",
        "neovim/neovim",
        "railwaycat/emacsmacport"
    ]
}
